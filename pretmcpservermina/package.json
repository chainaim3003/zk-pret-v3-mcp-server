{
  "name": "zkpret-mcp-server-mina",
  "version": "1.0.0",
  "description": "ZK-PRET MCP Server for Mina Protocol integration with zero-knowledge proof verification",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "zkpret-mcp-server": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc && npm run copy-data",
    "copy-data": "mkdir -p dist/data && cp -r src/data/* dist/data/ 2>/dev/null || true",
    "start": "node dist/index.js",
    "start:sse": "MCP_SERVER_MODE=sse PORT=3000 node dist/index.js",
    "dev": "ts-node-esm src/index.ts",
    "dev:sse": "MCP_SERVER_MODE=sse PORT=3000 ts-node-esm src/index.ts",
    "clean": "rm -rf dist",
    "test": "echo \"Add tests here\" && exit 0",
    "lint": "echo \"Add linting here\" && exit 0"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "zk-pret",
    "zero-knowledge",
    "mina-protocol",
    "o1js",
    "compliance",
    "verification",
    "blockchain"
  ],
  "author": "ChainAim Labs",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.7.0",
    "dotenv": "^16.5.0"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
